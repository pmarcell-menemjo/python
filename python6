import random
szamlalo=[0,0,0,0]
tomb=[random.randint(10,50)]
for i in range(1,100):
    tomb.append(random.randint(10,50))
print(tomb)

for i in range(1,100):
    for j in range(0,4):
        if tomb[i]==(j+1)*11:
            szamlalo[j]+=1
            tomb[i]=2*(j+1)
for k in range(0,4):
      print("A(z) %d számjegyből %d találat volt!" %((k+1)*11, szamlalo[k]))
      
